{
  "name": "katze",
  "version": "1.0.0",
  "description": "Sistema web completo para gestionar adopciones de gatos, con seguimiento post-adopci√≥n y contenido educativo.",
  "main": "index.js",
  "scripts": {
    "dev:back": "npm run dev --prefix backend",
    "dev:front": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run dev:back\" \"npm run dev:front\"",
    "setup": "npm install && cd backend && npm install && npm run migrate && npm run seed && cd ../frontend && npm install",
    "migrate": "npm run migrate --prefix backend",
    "seed": "npm run seed --prefix backend",
    "start:back": "npm start --prefix backend",
    "start:front": "npm run preview --prefix frontend"
  },
